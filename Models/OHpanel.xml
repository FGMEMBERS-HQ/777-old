	<effect>
		<inherits-from>Effects/model-transparent</inherits-from>
		<object-name>greentext</object-name>
		<object-name>greentext.001</object-name>
	</effect>

 	<animation>
		<type>material</type>
		<object-name>greentext</object-name>
		<object-name>greentext.001</object-name>
		<condition>
			 <greater-than>
				<property>systems/electrical/CPT-FLT-INST</property>
				<value>24.0</value>
			 </greater-than>
		</condition>
		<emission>
			<red>0.2</red>
			<green>0.6</green>
			<blue>0.2</blue>
			<factor-prop>controls/lighting/overhead-intencity</factor-prop>
		</emission>
	</animation>

