        <inherits-from>Effects/panel-light</inherits-from>
        <object-name>Panel.main</object-name>
        <object-name>Panel.Rwall</object-name>
        <object-name>Panel.Lwall</object-name>
        <object-name>Panel.center</object-name>
        <object-name>Pseat</object-name>
        <object-name>Pseat.armrest</object-name>
        <object-name>Cseat</object-name>
        <object-name>Cseat.armrest</object-name>
        <object-name>Seats</object-name>
        <object-name>Tray</object-name>
        <object-name>Yokeshaft</object-name>
