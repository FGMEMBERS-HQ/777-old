<PropertyList>

    <path>flightdeck.ac</path>

    <model>
        <path>Models/Aircraft/marker.xml</path>
    </model>

    <model>
        <name>Pedestal</name>
        <path>Aircraft/777/Models/pedestal.xml</path>
        <offsets>
            <x-m>-1.288</x-m>
            <y-m>0.017</y-m>
            <z-m>-0.388</z-m>
        </offsets>
    </model>

    <model>
        <name>EFB1</name>
        <path>Aircraft/777/Models/Instruments/EFB/EFB.xml</path>
        <offsets>
            <x-m>-1.506</x-m>
            <y-m>-1.034</y-m>
            <z-m>0.299</z-m>
            <pitch-deg>-27.0</pitch-deg>
            <heading-deg>38.0</heading-deg>
        </offsets>
    </model>

    <model>
        <name>EFB2</name>
        <path>Aircraft/777/Models/Instruments/EFB/EFB.xml</path>
        <offsets>
            <x-m>-1.506</x-m>
            <y-m>1.034</y-m>
            <z-m>0.299</z-m>
            <pitch-deg>-27</pitch-deg>
            <heading-deg>-38</heading-deg>
        </offsets>
    </model>

    <model>
        <name>OHpanel</name>
        <path>Aircraft/777/Models/OHpanel.xml</path>
        <offsets>
            <x-m>-1.281</x-m>
            <y-m>-0.007</y-m>
            <z-m>1.190</z-m>
            <pitch-deg>63</pitch-deg>
        </offsets>
    </model>

    <model>
        <name>APpanel</name>
        <path>Aircraft/777/Models/APpanel.xml</path>
        <offsets>
            <x-m>-1.824</x-m>
            <y-m>-0.050</y-m>
            <z-m>0.575</z-m>
            <pitch-deg>-10</pitch-deg>
        </offsets>
    </model>

    <model>
        <name>Panel3</name>
        <path>Aircraft/777/Models/Panel3.xml</path>
        <offsets>
            <x-m>-1.7839</x-m>
            <y-m>-0.3562</y-m>
            <z-m>0.2434</z-m>
            <pitch-deg>-44</pitch-deg>
        </offsets>
    </model>

     <model>
        <name>CTL1</name>
        <path>Aircraft/777/Models/Instruments/EFIS/efis-ctl.xml</path>
        <offsets>
            <x-m>-1.824</x-m>
            <y-m>-0.361</y-m>
            <z-m>0.575</z-m>
            <pitch-deg>-10</pitch-deg>
        </offsets>
    </model>

    <model>
        <name>CTL2</name>
        <path>Aircraft/777/Models/Instruments/EFIS/efis-ctl.xml</path>
        <offsets>
            <x-m>-1.824</x-m>
            <y-m>0.366</y-m>
            <z-m>0.575</z-m>
            <pitch-deg>-10</pitch-deg>
        </offsets>
    </model>

    <model>
        <name>P_MidSidePanel</name>
        <path>Aircraft/777/Models/L_MidSidePanel_Base.xml</path>
        <offsets>
            <x-m>-1.84</x-m>
            <y-m>-0.706</y-m>
            <z-m>0.385</z-m>
            <pitch-deg>-10</pitch-deg>
        </offsets>
    </model>

    <model>
        <name>CP_MidSidePanel</name>
        <path>Aircraft/777/Models/R_MidSidePanel_Base.xml</path>
        <offsets>
            <x-m>-1.84</x-m>
            <y-m>0.740</y-m>
            <z-m>0.385</z-m>
            <pitch-deg>-10</pitch-deg>
        </offsets>
    </model>

    <model>
        <name>Pclock</name>
        <path>Aircraft/777/Models/Instruments/chronometer.xml</path>
        <offsets>
            <x-m>-1.838</x-m>
            <y-m>-0.706</y-m>
            <z-m>0.417</z-m>
            <pitch-deg>-10</pitch-deg>
        </offsets>
    </model>

    <model>
        <name>CPclock</name>
        <path>Aircraft/777/Models/Instruments/chronometer.xml</path>
        <offsets>
            <x-m>-1.838</x-m>
            <y-m>0.740</y-m>
            <z-m>0.417</z-m>
            <pitch-deg>-10</pitch-deg>
        </offsets>
    </model>

    <model>
        <name>STDBY AI</name>
        <path>Aircraft/777/Models/Instruments/stby-ai.xml</path>
        <offsets>
            <x-m>-1.853</x-m>
            <y-m>-0.147</y-m>
            <z-m>0.465</z-m>
            <pitch-deg>-10</pitch-deg>
        </offsets>
    </model>

    <model>
        <name>STBY ASI</name>
        <path>Aircraft/777/Models/Instruments/stby-asi.xml</path>
        <offsets>
            <x-m>-1.837</x-m>
            <y-m>-0.147</y-m>
            <z-m>0.371</z-m>
            <pitch-deg>-10</pitch-deg>
        </offsets>
    </model>

    <model>
        <name>STBY ALT</name>
        <path>Aircraft/777/Models/Instruments/stby-alt.xml</path>
        <offsets>
            <x-m>-1.82</x-m>
            <y-m>-0.147</y-m>
            <z-m>0.277</z-m>
            <pitch-deg>-10</pitch-deg>
        </offsets>
    </model>

    <model>
        <name>GearCTL</name>
        <path>Aircraft/777/Models/gear-ctl.xml</path>
        <offsets>
            <x-m>-1.836</x-m>
            <y-m>0.182</y-m>
            <z-m>0.369</z-m>
            <pitch-deg>-10</pitch-deg>
        </offsets>
    </model>

    <model>
		<name>Dome light</name>
		<path>Aircraft/777/Models/Lights/dome_cone.xml</path>
		<offsets>
			<x-m>-1.16</x-m>
			<y-m>0</y-m>
			<z-m>0.9</z-m>
			<pitch-deg>-50</pitch-deg>
		</offsets>
	</model>

