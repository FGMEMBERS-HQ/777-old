<!--
777-200 MidSidePanel
-->

<PropertyList>
 
    <path>Panel3.ac</path>
    <text>
        <name>ShipIDL</name>
        <offsets>
            <x-m>0.003</x-m>
            <y-m>-0.0588</y-m>
            <z-m>-0.0054</z-m>
        </offsets>
        <alignment>center-center</alignment>
        <axis-alignment>yz-plane</axis-alignment>
        <type type="string">text-value</type>
        <property>sim/multiplay/callsign</property>
        <font>helvetica_bold.txf</font>
        <character-size>0.011</character-size>
        <character-aspect-ratio type="double">1.5</character-aspect-ratio>
        <font-resolution>
            <width type="int">64</width>
            <height type="int">64</height>
        </font-resolution>
    </text>
    
    <text>
        <name>CompanyIDL</name>
        <offsets>
            <x-m>0.003</x-m>
            <y-m>-0.0588</y-m>
            <z-m>0.0086</z-m>
        </offsets>
        <alignment>center-center</alignment>
        <axis-alignment>yz-plane</axis-alignment>
        <type type="string">text-value</type>
        <property>sim/aircraft-operator</property>
        <font>helvetica_bold.txf</font>
        <character-size>0.011</character-size>
        <character-aspect-ratio type="double">1.5</character-aspect-ratio>
        <font-resolution>
            <width type="int">64</width>
            <height type="int">64</height>
        </font-resolution>
    </text>
     <text>
         <name>ShipIDR</name>
        <offsets>
            <x-m>0.001</x-m>
            <y-m>0.805</y-m>
            <z-m>-0.0054</z-m>
        </offsets>
        <alignment>center-center</alignment>
        <axis-alignment>yz-plane</axis-alignment>
        <type type="string">text-value</type>
        <property>sim/multiplay/callsign</property>
        <font>helvetica_bold.txf</font>
        <character-size>0.011</character-size>
        <character-aspect-ratio type="double">1.5</character-aspect-ratio>
        <font-resolution>
            <width type="int">64</width>
            <height type="int">64</height>
        </font-resolution>
    </text>
    <text>
        <name>CompanyIDR</name>
        <offsets>
            <x-m>0.001</x-m>
            <y-m>0.805</y-m>
            <z-m>0.0086</z-m>
        </offsets>
        <alignment>center-center</alignment>
        <axis-alignment>yz-plane</axis-alignment>
        <type type="string">text-value</type>
        <property>sim/aircraft-operator</property>
        <font>helvetica_bold.txf</font>
        <character-size>0.011</character-size>
        <character-aspect-ratio type="double">1.5</character-aspect-ratio>
        <font-resolution>
            <width type="int">64</width>
            <height type="int">64</height>
        </font-resolution>
    </text>
     
     <animation>
        <type>material</type><!--white-->
        <object-name>ShipIDL</object-name>
        <object-name>ShipIDR</object-name>
        <object-name>CompanyIDL</object-name>
        <object-name>CompanyIDR</object-name>
       <emission>
            <red>0.1</red>
            <green>0.1</green>
            <blue>0.1</blue>
        </emission>
        <ambient>
            <red>1.0</red>
            <green>1.0</green>
            <blue>1.0</blue>
        </ambient>
        <diffuse>
            <red>0</red>
            <green>0</green>
            <blue>0</blue>
        </diffuse>
        <specular>
            <red>0</red>
            <green>0</green>
            <blue>0</blue>
        </specular>
    </animation>

</PropertyList>
